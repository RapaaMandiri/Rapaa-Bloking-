import tkinter as tk

# Create the main window
root = tk.Tk()

# Create a label
label = tk.Label(root, text="Label 1")
label.pack()

# Create a button
button = tk.Button(root, text="Tombol 1")
button.pack()

# Create a checkbox
checkbox = tk.Checkbutton(root, text="Centang 1")
checkbox.pack()

# Run the main loop
root.mainloop()
